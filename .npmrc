# NPM Configuration for TraceRoot

# Allow deprecated packages for blockchain tools
# These vulnerabilities are in dev-only dependencies (hardhat, fabric)
# and do not affect production code

# Disable audit for known unavoidable vulnerabilities in blockchain tools
audit=true
audit-level=moderate

# Use exact versions to ensure consistency
save-exact=false

# Set engine strict to false to allow flexible Node versions
engine-strict=false
